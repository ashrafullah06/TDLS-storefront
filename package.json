{
  "name": "my-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run prisma:generate:all && next build",
    "start": "next start",
    "db:ping": "dotenv -e .env -- node scripts/db/ping.mjs",
    "dev:app": "node scripts/dev-app.mjs",
    "build:app": "npm run prisma:generate:all && dotenv -e .env -- next build",
    "start:app": "dotenv -e .env -- next start",
    "app:studio": "dotenv -e .env -- npx prisma studio --schema=prisma/app/schema.prisma --hostname 127.0.0.1 --port 5555",
    "studio": "dotenv -e .env -- cross-env PRISMA_STUDIO_QUERY_ENGINE_TYPE=binary npx prisma studio --schema=prisma/app/schema.prisma --hostname 127.0.0.1 --port 5555",
    "prisma:studio:app": "dotenv -e .env -- npx prisma studio --schema=prisma/app/schema.prisma --port 5555",
    "app:push": "dotenv -e .env -- cross-env-shell \"set \\\"DATABASE_URL=%APP_DB_OWNER_DIRECT%\\\" && npx prisma db push --schema=prisma/app/schema.prisma\"",
    "app:migrate:create": "dotenv -e .env -- cross-env-shell \"set \\\"DATABASE_URL=%APP_DB_OWNER_DIRECT%\\\" && npx prisma migrate dev --schema=prisma/app/schema.prisma --create-only --name %npm_config_name%\"",
    "app:migrate:apply": "dotenv -e .env -- cross-env-shell \"set \\\"DATABASE_URL=%APP_DB_OWNER_DIRECT%\\\" && npx prisma migrate deploy --schema=prisma/app/schema.prisma\"",
    "app:migrate:status": "dotenv -e .env -- cross-env-shell \"set \\\"DATABASE_URL=%APP_DB_OWNER_DIRECT%\\\" && npx prisma migrate status --schema=prisma/app/schema.prisma\"",
    "app:reset": "dotenv -e .env -- cross-env-shell \"set \\\"DATABASE_URL=%APP_DB_OWNER_DIRECT%\\\" && npx prisma migrate reset --force --skip-seed --schema=prisma/app/schema.prisma\"",
    "app:gen": "prisma generate --schema=prisma/app/schema.prisma",
    "strapi:validate": "dotenv -e .env -- prisma validate --schema prisma/strapi/schema.prisma",
    "strapi:pull": "dotenv -e .env -- prisma db pull --schema prisma/strapi/schema.prisma",
    "strapi:format": "dotenv -e .env -- prisma format --schema prisma/strapi/schema.prisma",
    "strapi:gen": "prisma generate --schema prisma/strapi/schema.prisma",
    "strapi:sync": "npm run strapi:validate && npm run strapi:pull && npm run strapi:format && npm run strapi:gen",
    "strapi:studio:ro": "dotenv -e .env -- prisma studio --schema=prisma/strapi/schema.prisma",
    "prisma:generate:all": "npm run app:gen && npm run strapi:gen",
    "prisma:pull:strapi": "dotenv -e .env -- prisma db pull --schema=prisma/strapi/schema.prisma && dotenv -e .env -- prisma format --schema=prisma/strapi/schema.prisma && dotenv -e .env -- prisma generate --schema=prisma/strapi/schema.prisma",
    "prisma:gen:strapi": "dotenv -e .env -- prisma generate --schema=prisma/strapi/schema.prisma",
    "prisma:migrate:app": "dotenv -e .env -- prisma migrate dev --schema prisma/app/schema.prisma",
    "gql:sync": "npm run gql:pull:schema && npm run gql:auto:fragments && npm run gql:codegen",
    "gql:pull:schema": "node --env-file=.env.local ./scripts/refresh-schema.mjs",
    "gql:auto:fragments": "node ./scripts/auto-fragment.mjs",
    "gql:codegen": "dotenv -e .env.local -- graphql-codegen --config src/lib/graphql/codegen.yml",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "db:push:dev": "dotenv -e .env.development -- prisma db push",
    "db:migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "db:generate:dev": "dotenv -e .env.development -- prisma generate",
    "db:studio:dev": "dotenv -e .env.development -- prisma studio",
    "db:push:prod": "dotenv -e .env.production -- prisma db push",
    "db:migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "db:generate:prod": "dotenv -e .env.production -- prisma generate",
    "db:studio:prod": "dotenv -e .env.production -- prisma studio",
    "stock:sync:strapi": "dotenv -e .env -- node scripts/sync-prisma-stock-to-strapi.mjs",
    "preinstall": "npm config set ignore-scripts false",
    "postinstall": "npm run prisma:generate:all"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.10.0",
    "@emotion/react": "11.14.0",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.14.1",
    "@prisma/client": "6.14.0",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.6.1",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.8",
    "bcryptjs": "^3.0.3",
    "bwip-js": "^4.7.0",
    "clsx": "2.1.1",
    "cookie": "1.0.2",
    "docx": "^9.5.1",
    "framer-motion": "12.23.6",
    "hls.js": "^1.6.11",
    "html2canvas": "^1.4.1",
    "libphonenumber-js": "1.12.10",
    "lucide-react": "0.525.0",
    "mailersend": "2.6.0",
    "next": "15.3.4",
    "next-auth": "5.0.0-beta.29",
    "next-pwa": "5.6.0",
    "next-sitemap": "4.2.3",
    "nodemailer": "6.10.1",
    "openai": "5.12.0",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.1",
    "posthog-js": "1.259.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-barcode": "1.6.1",
    "react-dom": "19.1.0",
    "react-draggable": "4.5.0",
    "react-icons": "5.5.0",
    "react-intersection-observer": "9.16.0",
    "react-loading-skeleton": "3.5.0",
    "react-phone-number-input": "3.4.12",
    "react-qr-code": "2.0.18",
    "react-slick": "0.30.3",
    "react-toastify": "11.0.5",
    "recharts": "3.1.2",
    "slick-carousel": "1.8.1",
    "smoothscroll-polyfill": "0.4.4",
    "stripe": "^20.1.1",
    "swiper": "11.2.10",
    "swr": "2.3.4",
    "twilio": "5.8.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.0.0",
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/client-preset": "4.8.3",
    "@graphql-tools/load": "8.1.2",
    "@graphql-tools/url-loader": "8.0.33",
    "@graphql-tools/utils": "10.9.1",
    "@prisma/engines": "6.14.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/line-clamp": "0.4.4",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "0.5.16",
    "@types/node": "20.12.12",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "10.4.21",
    "cross-env": "10.0.0",
    "dotenv-cli": "7.4.4",
    "eslint": "9.7.0",
    "eslint-config-next": "15.3.4",
    "graphql": "16.11.0",
    "postcss": "8.5.6",
    "prisma": "6.14.0",
    "tailwind-scrollbar": "4.0.2",
    "tailwindcss": "^4.1.11",
    "typescript": "5.6.3"
  },
  "prisma": {
    "seed": "node prisma/app/seed.js"
  },
  "overrides": {
    "@prisma/engines": "6.14.0"
  },
  "engines": {
    "node": ">=18.17 <=21"
  },
  "packageManager": "npm@10.8.2"
}
